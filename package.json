{
  "name": "graasp",
  "version": "1.3.1",
  "engines": {
    "node": ">=14.0.0"
  },
  "description": "Graasp next level",
  "repository": {
    "type": "git",
    "url": "https://github.com/graasp/graasp"
  },
  "license": "AGPL-3.0-only",
  "author": "Graasp Association",
  "scripts": {
    "start": "yarn serve",
    "build": "yarn build-ts && yarn lint",
    "serve": "NODE_PATH=./dist node dist/server.js",
    "watch-node": "NODE_PATH=./dist nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "test": "DISABLE_LOGS=true jest --forceExit --verbose --runInBand",
    "test:ci": "DISABLE_LOGS=true jest --forceExit --verbose --coverage --silent --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "type-check": "tsc --noEmit",
    "lint": "eslint '**/*.{js,ts}'",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "check": "yarn prettier:check && yarn lint && yarn type-check",
    "debug": "yarn build && yarn watch-debug",
    "serve-debug": "NODE_PATH=./dist nodemon --inspect dist/server.js",
    "serve-debug-break": "NODE_PATH=./dist nodemon --inspect-brk dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn serve-debug\"",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "postinstall": "husky install",
    "pre-commit": "yarn prettier:check && yarn lint",
    "release": "standard-version -a",
    "release:first": "standard-version -a --first-release",
    "release:minor": "standard-version -a --release-as minor",
    "release:major": "standard-version -a --release-as major",
    "release:patch": "standard-version -a --release-as patch",
    "migration:create": "typeorm migration:create ./src/migrations/migrations",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate src/migrations/migrations -d src/plugins/datasource.ts && yarn build-ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d src/plugins/datasource.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d src/plugins/datasource.ts",
    "migration:drop": "npx typeorm-ts-node-commonjs schema:drop -d src/plugins/datasource.ts",
    "migration:fake": "yarn migration:run:fake && migration:revert:fake",
    "migration:run:fake": "npx typeorm-ts-node-commonjs  migration:run --fake",
    "migration:revert:fake": "npx typeorm-ts-node-commonjs migration:revert --fake",
    "migration:check": "npx typeorm-ts-node-commonjs migration:generate src/migrations/migrations --check -d src/plugins/datasource.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.306.0",
    "@aws-sdk/lib-storage": "3.379.1",
    "@aws-sdk/s3-request-presigner": "3.306.0",
    "@fastify/auth": "4.2.0",
    "@fastify/bearer-auth": "9.0.0",
