{
  "name": "graasp",
  "version": "0.5.2",
  "engines": {
    "node": ">=14.0.0"
  },
  "description": "Graasp next level",
  "repository": {
    "type": "git",
    "url": "https://github.com/graasp/graasp"
  },
  "license": "AGPL-3.0-only",
  "author": "Graasp Association",
  "scripts": {
    "start": "yarn serve",
    "build": "yarn build-ts && yarn lint && yarn copy-static-assets",
    "serve": "NODE_PATH=./dist node dist/server.js",
    "watch-node": "NODE_PATH=./dist nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "test": "DISABLE_LOGS=true jest --forceExit --verbose",
    "test:watch": "yarn test --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "copy-static-assets": "node copyStaticAssets.mjs",
    "debug": "yarn build && yarn watch-debug",
    "serve-debug": "NODE_PATH=./dist nodemon --inspect dist/server.js",
    "serve-debug-break": "NODE_PATH=./dist nodemon --inspect-brk dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn serve-debug\"",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "postinstall": "husky install",
    "pre-commit": "yarn prettier:check && yarn lint",
    "release": "standard-version -a",
    "release:first": "standard-version -a --first-release",
    "release:minor": "standard-version -a --release-as minor",
    "release:major": "standard-version -a --release-as major",
    "release:patch": "standard-version -a --release-as patch"
  },
  "dependencies": {
    "@casl/ability": "6.3.2",
    "@fastify/auth": "4.2.0",
    "@fastify/bearer-auth": "9.0.0",
    "@fastify/compress": "6.2.0",
    "@fastify/cors": "8.2.0",
    "@fastify/helmet": "10.1.0",
    "@fastify/jwt": "6.5.0",
    "@fastify/multipart": "7.4.0",
    "@fastify/secure-session": "6.0.0",
    "@fastify/static": "6.9.0",
    "@fastify/view": "7.4.1",
    "@graasp/translations": "github:graasp/graasp-translations#89-email",
    "@sentry/node": "7.39.0",
    "@sentry/profiling-node": "0.2.2",
    "@sentry/tracing": "7.39.0",
    "@types/sharp": "0.31.1",
    "add": "2.0.6",
    "archiver": "5.3.1",
    "aws-sdk": "2.1124.0",
    "bad-words": "3.0.4",
    "bcrypt": "5.0.1",
    "content-disposition": "0.5.4",
    "dotenv": "16.0.0",
    "eta": "2.0.0",
    "extract-zip": "2.0.1",
    "fastify": "4.10.2",
    "fastify-cors": "6.1.0",
    "fastify-nodemailer": "5.0.0",
    "fastify-plugin": "3.0.1",
    "fastify-secure-session": "3.2.0",
    "fluent-json-schema": "3.1.0",
    "form-data": "4.0.0",
    "http-status-codes": "2.2.0",
    "jsonwebtoken": "9.0.0",
    "lodash.groupby": "4.6.0",
    "lodash.uniqby": "4.7.0",
    "mime": "3.0.0",
    "mime-types": "2.1.35",
    "mmmagic": "0.5.3",
    "point-of-view": "6.3.0",
    "qs": "6.10.3",
    "sharp": "0.31.3",
    "slonik": "28.1.1",
    "slugify": "1.6.5",
    "striptags": "3.2.0",
    "tmp-promise": "3.0.3",
    "typeorm": "0.3.10",
    "uuid": "8.3.2",
    "yarn": "1.22.19"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/chai": "4.3.1",
    "@types/concurrently": "7.0.0",
    "@types/eslint": "8.4.1",
    "@types/jest": "29.4.0",
    "@types/mime": "3.0.1",
    "@types/node": "17.0.30",
    "@types/striptags": "3.1.1",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "chai": "4.3.6",
    "concurrently": "7.1.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "7.0.4",
    "jest": "29.4.1",
    "nodemon": "2.0.19",
    "prettier": "2.6.2",
    "shelljs": "0.8.5",
    "slonik-interceptor-query-logging": "1.4.7",
    "standard-version": "9.5.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "packageManager": "yarn@3.2.4",
  "resolutions": {
    "@graasp/sdk": "github:graasp/graasp-sdk#typeorm",
    "fastify": "4.10.2"
  }
}
