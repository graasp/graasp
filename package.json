    "@fastify/jwt": "6.7.1",
    "@fastify/multipart": "7.5.0",
    "@fastify/secure-session": "6.0.0",
    "@fastify/static": "6.10.2",
    "@fastify/view": "7.4.1",
    "@fastify/websocket": "7.2.0",
    "@graasp/etherpad-api": "2.1.1",
    "@graasp/sdk": "1.2.0",
    "@graasp/translations": "1.15.0",
    "@sentry/node": "7.64.0",
