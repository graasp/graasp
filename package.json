    "@types/chai": "4.3.1",
    "@types/concurrently": "7.0.0",
    "@types/eslint": "8.4.1",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.30",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "chai": "4.3.6",
    "concurrently": "7.1.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "nodemon": "2.0.19",
