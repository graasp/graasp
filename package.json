{
  "name": "graasp",
  "version": "0.1.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "description": "Graasp next level",
  "repository": {
    "type": "git",
    "url": "https://github.com/graasp/graasp"
  },
  "license": "AGPL-3.0-only",
  "author": "Graasp Association",
  "scripts": {
    "start": "yarn serve",
    "build": "yarn build-ts && yarn lint && yarn copy-static-assets",
    "serve": "NODE_PATH=./dist node dist/server.js",
    "watch-node": "NODE_PATH=./dist nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "test": "DISABLE_LOGS=true jest test --forceExit --coverage --verbose",
    "watch-test": "yarn test -- --watchAll",
    "build-ts": "tsc",
    "build-ts-declaration": "rm -rf ./types && tsc --project tsconfig.declaration.json --declaration --declarationDir ./types",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "copy-static-assets": "node copyStaticAssets.mjs",
    "debug": "yarn build && yarn watch-debug",
    "serve-debug": "NODE_PATH=./dist nodemon --inspect dist/server.js",
    "serve-debug-break": "NODE_PATH=./dist nodemon --inspect-brk dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn serve-debug\"",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "postinstall": "husky install",
    "pre-commit": "yarn prettier:check && yarn lint"
  },
  "dependencies": {
    "@graasp/translations": "github:graasp/graasp-translations",
    "aws-sdk": "^2.1051.0",
    "bcrypt": "5.0.1",
    "dotenv": "^10.0.0",
    "fastify": "^3.25.3",
    "fastify-auth": "^1.1.0",
    "fastify-bearer-auth": "^6.1.0",
    "fastify-compress": "^4.0.1",
    "fastify-cors": "^6.0.2",
    "fastify-helmet": "^5.3.2",
    "fastify-plugin": "^3.0.0",
    "fastify-secure-session": "^3.0.0",
    "fluent-json-schema": "3.0.1",
    "graasp-apps": "github:graasp/graasp-apps",
    "graasp-document-item": "github:graasp/graasp-document-item",
    "graasp-embedded-link-item": "github:graasp/graasp-embedded-link-item",
    "graasp-item-flagging": "github:graasp/graasp-item-flagging",
    "graasp-item-tags": "github:graasp/graasp-item-tags",
    "graasp-mailer": "github:graasp/graasp-mailer",
    "graasp-plugin-actions": "github:graasp/graasp-plugin-actions",
    "graasp-plugin-categories": "github:graasp/graasp-plugin-categories",
    "graasp-plugin-chatbox": "github:graasp/graasp-plugin-chatbox",
    "graasp-plugin-file": "github:graasp/graasp-plugin-file",
    "graasp-plugin-file-item": "github:graasp/graasp-plugin-file-item",
    "graasp-plugin-hidden-items": "github:graasp/graasp-plugin-hidden-items",
    "graasp-plugin-import-zip": "github:graasp/graasp-plugin-import-zip",
    "graasp-plugin-item-likes": "github:graasp/graasp-plugin-item-likes",
    "graasp-plugin-item-login": "github:graasp/graasp-plugin-item-login",
    "graasp-plugin-public": "github:graasp/graasp-plugin-public",
    "graasp-plugin-recycle-bin": "github:graasp/graasp-plugin-recycle-bin",
    "graasp-plugin-search": "github:graasp/graasp-plugin-search",
    "graasp-plugin-thumbnails": "github:graasp/graasp-plugin-thumbnails",
    "graasp-plugin-validation": "github:graasp/graasp-plugin-validation",
    "graasp-websockets": "github:graasp/graasp-websockets",
    "http-status-codes": "2.2.0",
    "jsonwebtoken": "^8.5.1",
    "qs": "^6.10.2",
    "slonik": "^27.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@types/chai": "^4.3.0",
    "@types/concurrently": "^6.4.0",
    "@types/eslint": "^8.2.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "chai": "^4.3.4",
    "concurrently": "^7.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "7.0.4",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "shelljs": "^0.8.4",
    "slonik-interceptor-query-logging": "^1.4.6",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.3.5"
  },
  "packageManager": "yarn@3.2.0-rc.3"
}
